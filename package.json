{
  "name": "wastemap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "firebase": "^5.9.2",
    "google-map-react": "1.1.2",
    "next": "^8.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
